<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
	<settings defaultResolver="central" />
	<resolvers>
		<ibiblio name="central" m2compatible="true" />
		<filesystem name="local">
			<artifact pattern="${ivy.settings.dir}/jars/lib/[organisation]/[module]/[revision]/[artifact]-[revision].[ext]" />
			<artifact pattern="${ivy.settings.dir}/../[artifact]/target/[artifact]-[revision].[ext]" />
		</filesystem>
	</resolvers>
	<modules>
		<module organisation="de.tudresden.inf.lat" name="jcel-coreontology" resolver="local"/>
		<module organisation="de.tudresden.inf.lat" name="jcel-core" resolver="local"/>
		<module organisation="de.tudresden.inf.lat" name="jcel-ontology" resolver="local"/>
		<module organisation="de.tudresden.inf.lat" name="jcel-reasoner" resolver="local"/>
		<module organisation="de.tudresden.inf.lat" name="jcel-owlapi" resolver="local"/>
		<module organisation="de.tudresden.inf.lat" name="jcel-protege" resolver="local"/>
	</modules>
</ivysettings>

