<?xml version="1.0" encoding="UTF-8"?>
<ivysettings>
	<settings defaultResolver="chain" />
	<resolvers>
		<chain name="chain" returnFirst="true">
			<filesystem name="local">
				<artifact pattern="${ivy.settings.dir}/jars/lib/[orgPath]/[module]/[revision]/[artifact]-[revision].[ext]" />
				<artifact pattern="${ivy.settings.dir}/../[artifact]/target/[artifact]-[revision].[ext]" />
			</filesystem>
			<ibiblio name="central" m2compatible="true" />
		</chain>
	</resolvers>
	<modules>
		<module organisation="de.tudresden.inf.lat.jcel" name="jcel-coreontology" resolver="local"/>
		<module organisation="de.tudresden.inf.lat.jcel" name="jcel-core" resolver="local"/>
		<module organisation="de.tudresden.inf.lat.jcel" name="jcel-ontology" resolver="local"/>
		<module organisation="de.tudresden.inf.lat.jcel" name="jcel-reasoner" resolver="local"/>
		<module organisation="de.tudresden.inf.lat.jcel" name="jcel-owlapi" resolver="local"/>
		<module organisation="de.tudresden.inf.lat.jcel" name="jcel-protege" resolver="local"/>
	</modules>
</ivysettings>

